{
    "name": "bakerkretzmar/bee.limo",
    "description": "Word game based on the New York Timesâ€™ Spelling Bee.",
    "type": "project",
    "license": "MIT",
    "homepage": "https://github.com/bakerkretzmar/bee.limo",
    "authors": [
        {
            "name": "Jacob Baker-Kretzmar",
            "email": "jacobtbk@gmail.com",
            "homepage": "https://github.com/bakerkretzmar"
        }
    ],
    "require": {
        "php": "^7.3",
        "fideloper/proxy": "^4.0",
        "inertiajs/inertia-laravel": "^0.1.3",
        "laravel/framework": "^8.0",
        "laravel/horizon": "^5.0",
        "laravel/tinker": "^2.0",
        "spatie/laravel-db-snapshots": "^1.3",
        "tightenco/ziggy": "^0.9.0"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan horizon:publish"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
